program Project1;

{$APPTYPE CONSOLE}

{$R *.res}

uses
  System.SysUtils;

var
  h : Real;

begin
  try
    WriteLn('insira sua altura:');
    ReadLn(h);
    WriteLn(Format('Masculino: %d',[72.7 * h]));
    WriteLn(Format('Feminino: %d',[62.1 * h]));
  except
    on E: Exception do
      Writeln(E.ClassName, ': ', E.Message);
  end;
end.
